<!DOCTYPE html>
<html>
<head>
  <title>UserDevise</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<%= flash.each do |key, value| %>
		<%= content_tag (:div,value,class: "alert aler-#{key}") %>
	<% end %>
	<%= yield %>
	<%= debug(params) if Rails.env.development? %>
</body>
</html>
